{
  "name": "visual-reporter",
  "version": "1.0.6",
  "description": "Generate Visual Changes Report",
  "bin": "dist/cmd.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir dist && chmod 755 dist/cmd.js",
    "help": "babel-node src/cmd.js --help",
    "doc": "babel-node src/cmd.js gen --baseline docs/baseline/ --compare docs/current/ --report docs --groups browsers,usecases,elements,screens --alpha 0.2 --customStyle '.card-content i{font-size:16px}.card-content a{display:inline-block;vertical-align:middle}.card-content li:before{content:\"â–¶\";color:#0ae}'",
    "lint": "prettier --write src/*.js;eslint src/*.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zordius/visual-reporter.git"
  },
  "keywords": [
    "visual-diff",
    "visual testing",
    "image",
    "screenshot",
    "testing",
    "html report"
  ],
  "author": "Zordius Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zordius/visual-reporter/issues"
  },
  "homepage": "https://github.com/zordius/visual-reporter#readme",
  "dependencies": {
    "cli-progress": "^2.1.1",
    "commander": "^2.20.0",
    "pixelmatch": "^5.0.2",
    "pngjs": "^3.4.0",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2"
  }
}
